{
  "identifier": "cursor-metrics-dashboard",
  "title": "üéØ Cursor AI Usage & Productivity Dashboard",
  "icon": "Code",
  "type": "dashboard",
  "widgets": [
    {
      "id": "overview-metrics",
      "type": "entities-number-chart",
      "title": "üìä Total Active Users",
      "description": "Number of active users using Cursor AI",
      "icon": "Users",
      "agentIdentifier": "cursor_usage_record",
      "blueprint": "cursor_usage_record",
      "dataset": {
        "combinator": "and",
        "rules": [
          {
            "operator": "=",
            "value": "cursor_usage_record",
            "property": "$blueprint"
          }
        ]
      },
      "chartType": "countEntities"
    },
    {
      "id": "total-accepts",
      "type": "entities-number-chart",
      "title": "‚úÖ Total AI Accepts",
      "description": "Total number of AI suggestions accepted",
      "icon": "Check",
      "agentIdentifier": "cursor_usage_record",
      "blueprint": "cursor_usage_record",
      "dataset": {
        "combinator": "and",
        "rules": [
          {
            "operator": "=",
            "value": "cursor_usage_record", 
            "property": "$blueprint"
          }
        ]
      },
      "chartType": "aggregateProperty",
      "property": "property#total_accepts",
      "func": "sum"
    },
    {
      "id": "total-cost",
      "type": "entities-number-chart",
      "title": "üí∞ Total Cost",
      "description": "Total cost of Cursor AI usage",
      "icon": "Dollar",
      "agentIdentifier": "cursor_usage_record",
      "blueprint": "cursor_usage_record",
      "dataset": {
        "combinator": "and",
        "rules": [
          {
            "operator": "=",
            "value": "cursor_usage_record",
            "property": "$blueprint"
          }
        ]
      },
      "chartType": "aggregateProperty",
      "property": "property#total_cost",
      "func": "sum"
    },
    {
      "id": "cursor-agent",
      "type": "ai-agent", 
      "title": "ü§ñ Cursor Assistant",
      "description": "AI assistant for analyzing cursor usage data",
      "icon": "Bot",
      "agentIdentifier": "cursor_usage_record"
    },
    {
      "id": "acceptance-rate-trend",
      "type": "line-chart",
      "title": "üìà AI Suggestion Acceptance Rate Over Time",
      "description": "Track how well users are accepting AI suggestions",
      "icon": "LineChart",
      "agentIdentifier": "cursor_usage_record",
      "blueprint": "cursor_usage_record",
      "chartType": "aggregateProperty",
      "property": "property#acceptance_rate",
      "func": "average",
      "timeInterval": "isoWeek",
      "timeRange": {
        "preset": "lastMonth"
      },
      "measureTimeBy": "$createdAt",
      "dataset": {
        "combinator": "and",
        "rules": [
          {
            "operator": "=",
            "value": "cursor_usage_record",
            "property": "$blueprint"
          }
        ]
      }
    },
